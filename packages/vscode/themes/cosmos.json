	"semanticTokenColors": {
		"enum": {
			"foreground": "#FDFFB6"
		},
		"enumMember": {
			"foreground": "#73CAD4"
		},
		"property": {
			"foreground": "#dc8a78"
		},
		"parameter": {
			"foreground": "#F1EDF1"
		},
		"interface": {
			"foreground": "#B4A180"
		},
		"typeParameter": {
			"foreground": "#FFC2A9"
		},
		"function": {
			"foreground": "#A0C4FF"
		},
		"method": {
			"foreground": "#A0C4FF"
		},
		"lifetime": {
			"foreground": "#E36D6D",
			"fontStyle": "italic"
		},
		"namespace": {
			"foreground": "#FFC6FF"
		},
		"macro": {
			"foreground": "#5AABE4",
			"fontStyle": "italic"
