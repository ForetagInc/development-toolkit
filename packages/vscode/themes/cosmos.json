			"foreground": "#E1A679"
		},
		"string": {
			"foreground": "#FF6A99"
		},
		"boolean": {
			"foreground": "#FF6A99"
		}
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7B797B"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Primitives",
			"scope": [
				"string.template.ts",
				"string.quoted.single.tsx",
				"string.quoted.single.ts",
				"constant.numeric.decimal.ts"
			],
			"settings": {
				"foreground": "#FF61AB"
			}
		},
		{
			"name": "Type",
			"scope": [
				"support.type.primitive.ts"
			],
			"settings": {
				"foreground": "#E1A679"
			}
		},
		{
			"name": "Punctuation - Brace",
			"scope": [
				"punctuation.definition.binding-pattern.object.ts",
				"punctuation.definition.binding-pattern.array.ts",
				"punctuation.definition.block.tsx",
				"meta.brace.square.ts"
			],
			"settings": {
				"foreground": "#f9e2af"
			}
		},
		{
			"name": "Entities / Classes / Decorator / Structs",
			"scope": [
				"entity.name.tag.tsx",
				"entity.name.type.class.tsx",
				"meta.decorator.tsx",
				"entity.name.type.struct.rust"
			],
			"settings": {
				"foreground": "#FDFFB6"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"storage.type.tsx",
				"keyword.control.export.tsx",
				"keyword.control.import.tsx",
				"storage.type.class.tsx"
			],
			"settings": {
				"foreground": "#FF61AB"
			}
		},
		{
			"name": "Namespace / Module",
			"scope": [
				"meta.import.tsx",
				"meta.import.ts"
			],
			"settings": {
				"foreground": "#FF61AB"
			}
		},
		{
			"name": "Block",
			"scope": [
				"meta.block.tsx",
				"meta.block.ts"
			],
			"settings": {
				"foreground": "#FFC6FF"
			}
		},
		// TS
		{
			"name": "Punctuation - Default",
			"scope": [
				"meta.brace.round.ts"
			],
			"settings": {
				"foreground": "#F1EDF1"
			}
		},
		{
			"name": "TS - Native objects",
			"scope": [
				"variable.other.object.ts"
			],
			"settings": {
				"foreground": "#FCA669"
			}
		},
		{
			"name": "TS - Builtins",
			"scope": [
				"support.type.builtin.ts",
				"constant.language.null.ts",
				"constant.language.undefined.ts"
			],
			"settings": {
				"foreground": "#6c7086"
			}
		},
		{
			"name": "TS - TypeOf expression",
			"scope": [
				"keyword.operator.expression.typeof.ts"
			],
			"settings": {
				"foreground": "#9399b2"
			}
		},
		{
			"name": "TS - KeyOf expression",
			"scope": [
				"keyword.operator.expression.keyof.ts"
			],
			"settings": {
				"foreground": "#6c7086"
			}
		},
		{
			"name": "TS - as Keyword",
			"scope": [
				"keyword.control.as.ts"
			],
			"settings": {
				"foreground": "#f5c2e7"
			}
		},
		{
			"name": "TS - return Keyword",
			"scope": [
				"keyword.control.flow.ts"
			],
			"settings": {
				"foreground": "#737994"
			}
		},
		{
			"name": "TS - Variable / Constants - Keyword",
			"scope": [
				"storage.type.ts"
			],
			"settings": {
				"foreground": "#cad3f5"
			}
		},
		{
			"name": "TS - Constant",
			"scope": [
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#e78284"
			}
		},
		{
			"name": "TS - Variable",
			"scope": [
				"meta.definition.variable.ts",
				"variable.other.readwrite.ts"
			],
			"settings": {
				"foreground": "#94e2d5"
			}
		},
		{
			"name": "TS - Modifier",
			"scope": [
				"storage.modifier.tsx"
			],
			"settings": {
				"foreground": "#E1A679"
			}
		},
		{
			"name": "TS - Export",
			"scope": [
				"keyword.control.export.ts"
			],
			"settings": {
				"foreground": ""
			}
		},
		{
			"name": "TSX - Value",
			"scope": [
				"constant.language.null.tsx"
			],
			"settings": {
				"foreground": "#FF6A99"
			}
		},
		// Rust
	]
