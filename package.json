{
	"name": "development-toolkit",
	"displayName": "Foretag Development Toolkit",
	"description": "Foretag themes, tools & extensions for VS Code",
	"keywords": [
		"env",
		"dotenv",
		"foretag"
	],
	"publisher": "foretag",
	"icon": "images/foretag_logo_yellow.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/ForetagInc/vscode-toolkit.git"
	},
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.66.1"
	},
	"categories": [
		"Programming Languages",
		"Themes",
		"Extension Packs"
	],
	"extensionPack": [
		"matklad.rust-analyzer",
		"serayuzgur.crates",
		"vadimcn.vscode-lldb",
		"masterco.master-styles-language-service",
		"eg2.vscode-npm-script",
		"tamasfe.even-better-toml",
		"redhat.vscode-xml",
		"redhat.vscode-yaml",
		"editorconfig.editorconfig",
		"please-build.plz-vscode",
		"ms-kubernetes-tools.vscode-kubernetes-tools",
		"ms-azuretools.vscode-docker"
	],
	"contributes": {
		"languages": [
			{
				"id": "dotenv",
				"aliases": [
					"Environment Variables",
					"dotenv",
					"env"
				],
				"extensions": [
					".flaskenv",
					".env",
					".env-sample",
					".env.example",
					".env.template",
					".env.local",
					".env.dev",
					".env.dev.local",
					".env.build",
					".env.build.local",
					".env.development",
					".env.development.local",
					".env.test",
					".env.test.local",
					".env.testing",
					".env.qa",
					".env.qa.local",
					".env.uat",
					".env.uat.local",
					".env.stag",
					".env.stag.local",
					".env.stage",
					".env.stage.local",
					".env.staging",
					".env.staging.local",
					".env.live",
					".env.live.local",
					".env.production",
					".env.production.local",
					".env.prod",
					".env.prod.local"
				],
				"filenamePatterns": [
					"?*env"
				],
				"filenames": [
					"env"
				],
				"configuration": "./languages/env/language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "dotenv",
				"scopeName": "source.env",
				"path": "./languages/env/syntaxes/env.tmLanguage"
			}
		],
		"themes": [
			{
				"label": "Foretag Indigo",
				"uiTheme": "vs-dark",
				"path": "./themes/Foretag : Indigo-color-theme.json"
			}
		]
	}
}